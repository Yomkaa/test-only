local noclipKey = Enum.KeyCode.N
local noclipToggleConnection = nil
local function setupNoclipKeybindListener()
if noclipToggleConnection then
noclipToggleConnection:Disconnect()
noclipToggleConnection = nil
end
noclipToggleConnection = UserInputService.InputBegan:Connect(function(input, gameProcessed)
if gameProcessed then return end
if input.KeyCode == noclipKey then
flags.noClip = not flags.noClip
if flags.noClip then
enableNoclip()
Notification.new({
Title = __B64__(__B64__('VG05RGJHbHdJRVZ1WVdKc1pXUT0=')),
Description = __B64__(__B64__('Ulc1cWIzaz0=')),
Duration = 3,
Icon = __B64__(__B64__('WW14bGJtUT0='))
})
else
disableNoclip()
Notification.new({
Title = __B64__(__B64__('VG05RGJHbHdJRVJwYzJGaWJHVms=')),
Description = __B64__(__B64__('VG05amJHbHdJR1JwYzJGaWJHVms=')),
Duration = 3,
Icon = __B64__(__B64__('Ykc5amF3PT0='))
})
end
end
end)
end
setupNoclipKeybindListener()
MovementSettingsSection:NewKeybind({
Title = __B64__(__B64__('VG05RGJHbHdJRXRsZVdKcGJtUT0=')),
Name = __B64__(__B64__('VG05RGJHbHdTMlY1WW1sdVpBPT0=')),
Default = Enum.KeyCode.N,
Description = __B64__(__B64__('UzJWNUlIUnZJSFJ2WjJkc1pTQk9iME5zYVhBPQ==')),
Callback = function(key)
if typeof(key) == __B64__(__B64__('Ulc1MWJVbDBaVzA9')) then
noclipKey = key
elseif typeof(key) == __B64__(__B64__('YzNSeWFXNW4=')) then
local upper = key:upper()
local suc, enumKey = pcall(function() return Enum.KeyCode[upper] end)
if suc and enumKey then
noclipKey = enumKey
end
end
setupNoclipKeybindListener()
end
})
