local LightingSettingsSection = MiscTab:NewSection({
Title = __B64__(__B64__('VEdsbmFIUnBibWNnVTJWMGRHbHVaM009')),
Icon = __B64__(__B64__('YzNWdQ==')),
Position = __B64__(__B64__('VEdWbWRBPT0='))
})
LightingSettingsSection:NewToggle({
Title = __B64__(__B64__('Um5Wc2JFSnlhV2RvZEE9PQ==')),
Name = __B64__(__B64__('Um5Wc2JFSnlhV2RvZEE9PQ==')),
Default = false,
Callback = function(state)
local L = game:GetService(__B64__(__B64__('VEdsbmFIUnBibWM9')))
if not flags.fullBrightInit then
NormalLightingSettings = {
Brightness = L.Brightness,
ClockTime = L.ClockTime,
FogEnd = L.FogEnd,
GlobalShadows = L.GlobalShadows,
Ambient = L.Ambient
}
for _, prop in ipairs({__B64__(__B64__('UW5KcFoyaDBibVZ6Y3c9PQ==')), __B64__(__B64__('UTJ4dlkydFVhVzFs')), __B64__(__B64__('Um05blJXNWs=')), __B64__(__B64__('UjJ4dlltRnNVMmhoWkc5M2N3PT0=')), __B64__(__B64__('UVcxaWFXVnVkQT09'))}) do
L:GetPropertyChangedSignal(prop):Connect(function()
if flags.fullBright then
if prop == __B64__(__B64__('UVcxaWFXVnVkQT09')) then L.Ambient = Color3.fromRGB(255,255,255)
elseif prop == __B64__(__B64__('UjJ4dlltRnNVMmhoWkc5M2N3PT0=')) then L.GlobalShadows = false
elseif prop == __B64__(__B64__('Um05blJXNWs=')) then L.FogEnd = 1000000
elseif prop == __B64__(__B64__('UTJ4dlkydFVhVzFs')) then L.ClockTime = 12
elseif prop == __B64__(__B64__('UW5KcFoyaDBibVZ6Y3c9PQ==')) then L.Brightness = 10 end
end
end)
end
flags.fullBrightInit = true
end
flags.fullBright = state
if state then
L.Brightness = 10
L.ClockTime = 12
L.FogEnd = 1000000
L.GlobalShadows = false
L.Ambient = Color3.fromRGB(255,255,255)
else
L.Brightness = NormalLightingSettings.Brightness
L.ClockTime = NormalLightingSettings.ClockTime
L.FogEnd = NormalLightingSettings.FogEnd
L.GlobalShadows = NormalLightingSettings.GlobalShadows
L.Ambient = NormalLightingSettings.Ambient
end
end
})
local doorESPMap = {}
local doorESPAddedConn = nil
local doorESPRemovingConn = nil
local function createDoorESP(door)
if not door or doorESPMap[door] then return end
local adorneePart = nil
if door:IsA(__B64__(__B64__('UW1GelpWQmhjblE9'))) then
adorneePart = door
else
adorneePart = door:FindFirstChildWhichIsA(__B64__(__B64__('UW1GelpWQmhjblE9'))) or door.PrimaryPart
end
if not adorneePart then
for _, c in ipairs(door:GetChildren()) do
if c:IsA(__B64__(__B64__('UW1GelpWQmhjblE9'))) then
adorneePart = c
break
end
end
end
if not adorneePart then return end
local playerGui = player:FindFirstChild(__B64__(__B64__('VUd4aGVXVnlSM1Zw'))) or player:WaitForChild(__B64__(__B64__('VUd4aGVXVnlSM1Zw')))
local billboard = Instance.new(__B64__(__B64__('UW1sc2JHSnZZWEprUjNWcA==')))
billboard.Name = __B64__(__B64__('V1c5NGFVUnZiM0pGVTFBPQ=='))
billboard.Adornee = adorneePart
billboard.AlwaysOnTop = true
billboard.Size = UDim2.new(0, 80, 0, 20)
billboard.StudsOffset = Vector3.new(0, 1.6, 0)
billboard.Parent = playerGui
local frame = Instance.new(__B64__(__B64__('Um5KaGJXVT0=')))
frame.Size = UDim2.new(1, 0, 1, 0)
frame.BackgroundTransparency = 0.2
frame.BackgroundColor3 = Color3.fromRGB(138, 43, 226) 
frame.BorderSizePixel = 1
frame.BorderColor3 = Color3.fromRGB(255, 255, 255)
frame.Parent = billboard
local uicorner = Instance.new(__B64__(__B64__('VlVsRGIzSnVaWEk9')))
uicorner.CornerRadius = UDim.new(0,6)
uicorner.Parent = frame
local label = Instance.new(__B64__(__B64__('VkdWNGRFeGhZbVZz')))
label.Size = UDim2.new(1, -8, 1, 0)
label.Position = UDim2.new(0, 4, 0, 0)
label.BackgroundTransparency = 1
label.Text = __B64__(__B64__('Ukc5dmNnPT0='))
label.TextColor3 = Color3.fromRGB(255,255,255)
label.Font = Enum.Font.GothamBold
label.TextScaled = true
label.TextStrokeTransparency = 0.6
label.Parent = frame
doorESPMap[door] = billboard
local conn
conn = Workspace.DescendantRemoving:Connect(function(obj)
if obj == door or obj == adorneePart then
if doorESPMap[door] and doorESPMap[door].Parent then
doorESPMap[door]:Destroy()
end
doorESPMap[door] = nil
if conn then conn:Disconnect() end
end
end)
end
local function removeDoorESP(door)
if not door then return end
if doorESPMap[door] then
pcall(function() doorESPMap[door]:Destroy() end)
doorESPMap[door] = nil
end
end
local function isDoorRemote(remoteEvent)
if not remoteEvent or not remoteEvent:IsA(__B64__(__B64__('VW1WdGIzUmxSWFpsYm5RPQ=='))) then return false end
if remoteEvent.Parent and remoteEvent.Parent.Name == __B64__(__B64__('Ukc5dmNnPT0=')) and remoteEvent.Parent.Parent and remoteEvent.Parent.Parent.Parent == Workspace.Rooms then
return true
end
return false
end
local function enableDoorESP()
if doorESPAddedConn or doorESPRemovingConn then return end
for _, obj in ipairs(Workspace:GetDescendants()) do
if obj:IsA(__B64__(__B64__('VW1WdGIzUmxSWFpsYm5RPQ=='))) and isDoorRemote(obj) then
local doorModel = obj.Parent
createDoorESP(doorModel)
end
end
doorESPAddedConn = Workspace.DescendantAdded:Connect(function(obj)
if obj:IsA(__B64__(__B64__('VW1WdGIzUmxSWFpsYm5RPQ=='))) and isDoorRemote(obj) then
local doorModel = obj.Parent
createDoorESP(doorModel)
elseif obj:IsA(__B64__(__B64__('VFc5a1pXdz0='))) and obj.Name == __B64__(__B64__('Ukc5dmNnPT0=')) and obj.Parent and obj.Parent.Parent == Workspace.Rooms then
createDoorESP(obj)
end
end)
doorESPRemovingConn = Workspace.DescendantRemoving:Connect(function(obj)
if obj:IsA(__B64__(__B64__('VW1WdGIzUmxSWFpsYm5RPQ=='))) and isDoorRemote(obj) then
local doorModel = obj.Parent
removeDoorESP(doorModel)
elseif obj:IsA(__B64__(__B64__('VFc5a1pXdz0='))) and obj.Name == __B64__(__B64__('Ukc5dmNnPT0=')) and obj.Parent and obj.Parent.Parent == Workspace.Rooms then
removeDoorESP(obj)
end
end)
end
local function disableDoorESP()
for door, billboard in pairs(doorESPMap) do
removeDoorESP(door)
end
doorESPMap = {}
if doorESPAddedConn then
doorESPAddedConn:Disconnect()
doorESPAddedConn = nil
end
if doorESPRemovingConn then
doorESPRemovingConn:Disconnect()
doorESPRemovingConn = nil
end
end
local ESPSection = MiscTab:NewSection({
Title = __B64__(__B64__('UlZOUUlGTmxkSFJwYm1keg==')),
Icon = __B64__(__B64__('WlhsbA==')),
Position = __B64__(__B64__('VW1sbmFIUT0='))
})
ESPSection:NewToggle({
Title = __B64__(__B64__('Ukc5dmNpQkZVMUE9')),
Name = __B64__(__B64__('Ukc5dmNrVlRVQT09')),
Default = false,
Callback = function(state)
flags.doorESP = state
if state then
enableDoorESP()
else
disableDoorESP()
end
end
})
