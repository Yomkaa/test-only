local function getCurrentRoom()
local char = player.Character
if not char or not char:FindFirstChild(__B64__(__B64__('U0hWdFlXNXZhV1JTYjI5MFVHRnlkQT09'))) then return nil end
for _, room in ipairs(Workspace.Rooms:GetChildren()) do
local ent = room:FindFirstChild(__B64__(__B64__('Ulc1MGNtRnVZMlU9')))
if ent and (char.HumanoidRootPart.Position - ent.Position).Magnitude < 10 then
return room
end
end
return nil
end
